{

  "redundant_nodes" : {
    "pattern" : [".", ",", ")", "(",  "[", "]", ":", ";", "*", "+", "=", "-", "\"", "$", "<-", "\n", "null", "%"],
    "rule" : ["inject_remove_node", "node"]
  },

  "node_transformation1" : {
    "pattern": [
      "expression_statement -> assignment",
      "assignment -> variable_list",
      "assignment -> call",
      "call -> caller_function"
    ],
    "rule": {
      "inject_remove_node": "expression_statement",
      "inject_remove_node": "assignment",
      "inject_add_edge" : ["caller_function", "variable_list"]
    }
  },

}